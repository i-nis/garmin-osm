# Argentina (ARG)

highway=trunk & mkgmap:country=ARG	{ add bicycle=no; add foot=no }
highway=cycleway & mkgmap:country=ARG	{ add foot=no }
highway=bridleway & mkgmap:country=ARG	{ add foot=yes }

